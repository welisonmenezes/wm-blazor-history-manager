<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Welison Menezes</Authors>
    <Description>Add Back and Foward links for your Blazor Wasm or Blazor Server aplication. It is a Blazor history API implementation using either native HTML5 History API or custom behaviors implementation with more features.</Description>
    <Copyright>MIT</Copyright>
    <PackageProjectUrl>https://welisonmenezes.github.io/wm-blazor-history-manager/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/welisonmenezes/wm-blazor-history-manager</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>history forward history-api back history-manager blazor-history-manager blazor-back blazor-forward blazor-history-api</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.0" />
  </ItemGroup>

</Project>
